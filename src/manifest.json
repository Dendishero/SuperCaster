{
	"manifest_version": 2,
	"name": "SuperCaster",
	"version": "1.3",
	"description": "Allows more types of videos to be chromecasted",
	"background": {
		"persistent": true,
		"scripts": ["background.js"]
	},
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	],
	"background": {
		"scripts": ["content_script.js"],
		"persistent": false
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup":"popup.html"
	}
}